{
  "name": "c8y-pkg-bookmark-plugin",
  "version": "1.0.0",
  "description": "Allows you to bookmark your favorite device in the navigator.",
  "scripts": {
    "start": "c8ycli server -u http://jan.latest.stage.c8y.io --shell cockpit",
    "build": "c8ycli build",
    "deploy": "c8ycli deploy",
    "postinstall": "ngcc"
  },
  "keywords": [
    "Cumulocity",
    "Plugin"
  ],
  "author": "Jan Hommes",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "14.0.6",
    "@angular/cdk": "14.1.2",
    "@angular/common": "14.0.6",
    "@angular/compiler": "14.0.6",
    "@angular/core": "14.0.6",
    "@angular/forms": "14.0.6",
    "@angular/platform-browser": "14.0.6",
    "@angular/platform-browser-dynamic": "14.0.6",
    "@angular/router": "14.0.6",
    "@angular/upgrade": "14.0.6",
    "@c8y/client": "~1016.0.0",
    "@c8y/ngx-components": "~1016.0.0",
    "@ngx-translate/core": "14.0.0",
    "rxjs": "~6.6.3",
    "zone.js": "~0.11.7",
    "@c8y/style": "~1016.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.0.6",
    "@angular/compiler-cli": "14.0.6",
    "@angular/language-service": "14.0.6",
    "@angular/service-worker": "14.0.6",
    "@angular/localize": "14.0.6",
    "@types/jest": "^28.1.6",
    "jest": "^28.1.3",
    "jest-preset-angular": "^12.2.0",
    "typescript": "4.7.4",
    "@c8y/cli": "~1016.247.0"
  },
  "c8y": {
    "application": {
      "name": "Bookmark plugin",
      "description": "Allows you to bookmark your favorite device in the navigator.",
      "contextPath": "sag-ps-pkg-bookmark-plugin",
      "key": "sag-ps-pkg-bookmark-plugin-key",
      "tabsHorizontal": true,
      "isPackage": true,
      "noAppSwitcher": true,
      "package": "plugin",
      "exports": [
        {
          "name": "Device bookmarks",
          "module": "BookmarksModule",
          "path": "./bookmarks/bookmarks.module.ts",
          "description": "Allows you to bookmark your favorite device in the navigator."
        }
      ],
      "remotes": {
        "c8y-pkg-bookmark-plugin": [
          "BookmarksModule"
        ]
      },
      "copy": [
        { "from": "img/bookmark-plugin.gif", "to": "img/bookmark-plugin.gif" },
        { "from": "LICENCE", "to": "LICENCE" }
      ]
    }
  },
  "browserslist": [
    "last 2 major versions"
  ]
}
